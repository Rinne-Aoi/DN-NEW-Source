<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >


    <TextView
        android:id="@+id/setting3_summary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/setting3_title"
        android:text="@string/setting3_summary" />

    <View
        android:id="@+id/View02"
        android:layout_width="fill_parent"
        android:layout_height="2sp"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/setting3_summary"
        android:background="#CCCCCC" />

    <TextView
        android:id="@+id/setting4_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/View02"
        android:text="@string/setting4_title"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/setting4_summary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/setting4_title"
        android:text="@string/setting4_summary" />

    <View
        android:id="@+id/View03"
        android:layout_width="fill_parent"
        android:layout_height="2sp"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/setting4_summary"
        android:background="#CCCCCC" />

    <Button
        android:id="@+id/go1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/setting3_title"
        android:background="#00000000"
        android:text="@string/go"
        android:textSize="11pt" />

    <Button
        android:id="@+id/go2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/View02"
        android:background="#00000000"
        android:text="@string/go"
        android:textSize="11pt" />

    <TextView
        android:id="@+id/setting3_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/linearLayout1"
        android:text="@string/setting3_title"
        android:textAppearance="?android:attr/textAppearanceLarge" />

</RelativeLayout>